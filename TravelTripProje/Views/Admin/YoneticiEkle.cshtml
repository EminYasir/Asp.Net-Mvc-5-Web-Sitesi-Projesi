@model TravelTripProje.Models.Siniflar.Admin
@{
    ViewBag.Title = "YoneticiEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Yönetici Ekleme</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Kullanici)
    @Html.TextBoxFor(x => x.Kullanici, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Sifre)
    @Html.TextBoxFor(x => x.Sifre, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary" disabled>Ekle</button>
</form>

