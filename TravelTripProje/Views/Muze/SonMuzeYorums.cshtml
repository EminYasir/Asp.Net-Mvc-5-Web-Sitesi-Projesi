@using TravelTripProje.Models.Siniflar
@model List<MuzeYorumlar>

<div class="comments">
    <h3>Son Yorumlar</h3>
    <ul>
        @foreach (var x in Model)
        {
            <li><a href="/Muze/MuzeDetails/@x.Muzeid">@x.Yorum.Substring(0, 15)...</a> -> <a href="/Muze/MuzeDetails/@x.Muzeid">@x.Otel.Baslik.Substring(0, 25)...</a></li>
        }

    </ul>
</div>



